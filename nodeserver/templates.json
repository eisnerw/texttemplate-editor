	"web": [
		"Here is an example of tagging and styling using Text Templates:\\n",
		"{[Hello {'test'.#Style('outlined-red-dots')} `\\n",
		"{\"World\".#Style('italic','bold','red')}!!] \\n",
		".#Style('normal','blue').#Tag('h2').#ToHtml()} \\n",
		"{\\n",
		"   'Testing'.#Click(\\n",
		"         'simple'  // the link name\\n",
		"         ,'Click this to load a simple script' // the title (hover over the link)\\n",
		"         , 'link','italic' // style attributes\\n",
		"      )\\n",
		"} is what we do\\n",
		"\\n",
		"{#external}\\n",
		"Subtemplates:\\n",
		"{#Style: \\n",
		"[<span style=\"{$1.#StyleCases(\\n",
		"   $2,$3,$4,$5,$6,$7,$8,$9  \\n",
		")}\">{$0}</span>]} \\n",
		"{#ToHtml:[<pre><span>{$0}</span><pre>]}\\n",
		"{#Tag:[<{$1}>{$0}</{$1}>]}\\n",
		"{#Click:[<a title='{$2=>[{$2}],[Click this]}' `\\n",
		"   onclick=\"document.location='/#{$1}'; `\\n",
		"   document.location.reload()\">{$0}</a>]\\n",
		"   .#OptionalStyle('link',$2,$3,$4,$5,$6,$7,$8,$9)}\\n",
		"{#StyleCases:[ `\\n",
		"   {$0\\n",
		"      .Case(\\n",
		"         'italic','font-style:italic'\\n",
		"         ,'normal','font-weight:normal;white-space:normal'\\n",
		"         ,'bold','font-weight:bold'\\n",
		"         ,'red','color:red'\\n",
		"         ,'blue','color:blue'\\n",
		"         ,'black','color:black'\\n",
		"         ,'outlined-red-dots','outline-color:red;outline-style:dotted'\\n",
		"         ,'underline','text-decoration: underline'\\n",
		"         ,'link','text-decoration: underline;color:blue'\\n",
		"      )\\n",
		"   };\\n",
		"   {$1->[{$1.#StyleCases($2,$3,$4,$5,$6,$7,$8,$9)}]}\\n",
		"]}\\n",
		"{#OptionalStyle:[{$2=>[\\n",
		"      {$0.#Style($2,$3,$4,$5,$6,$7,$8,$9)}\\n",
		"   ],[\\n",
		"      {$0.#Style($1)}]}\\n",
		"]}\\n",
		"{#external:[\\n",
		"This is an example of a link to an external data source (in this case, Github)\\n",
		"\\n",
		"my repos: {'https://api.github.com/users/eisnerw/repos': #formatGitRepos}\\n",
		"Subtemplates:    \\n",
		"\\n",
		"{#formatGitRepos:[- {name}{description->[ ({description})]}]} \\n",
		"]}"
